<record id="collection-object" is-extension="true" generate-services-schema="true">
	<services-record-path id="variablemedia">
		collectionobjects_variablemedia:http://collectionspace.org/services/collectionobject/domain/collectionobject,collectionobjects_variablemedia
	</services-record-path>

	<section id="descriptionInformation">
		<!-- renaming contentWorksList to vmContentWorksList -->
		<repeat id="vmContentWorksList/vmContentWorksGroup" section="variablemedia">
			<field id="contentWork" section="variablemedia" autocomplete="true" />
			<field id="contentWorkType" section="variablemedia" autocomplete="true" ui-type="enum" />
			<field id="contentWorkNote" section="variablemedia" />
		</repeat>
	</section>

	<section id="technicalSpecs" section="variablemedia">
		<repeat id="filmVideoComponentsList/filmVideoComponentsGroup" section="variablemedia">
			<field id="originalFormat" section="variablemedia" autocomplete="true" ui-type="enum" />
			<field id="originalAudioFormat" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="copiesProvided" section="variablemedia">
			<field id="copyProvidedOnAcquisition" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="digitalWorksList/digitalWorksGroup" section="variablemedia">
			<field id="programmingLanguage" section="variablemedia" autocomplete="true" ui-type="enum" />
			<field id="authoringEnvironment" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="compressionSystems" section="variablemedia">
			<field id="compressionSystem" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="ratioFormats" section="variablemedia">
			<field id="ratioFormat" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>

		<field id="nonStandardCode" section="variablemedia" />
		<field id="sourceExternalData" section="variablemedia" />

		<repeat id="visualPreferencesList/visualPreferencesGroup" section="variablemedia">
			<field id="screenResolution" section="variablemedia" autocomplete="true" ui-type="enum" />
			<field id="colorPalette" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="creationHardwares" section="variablemedia">
			<field id="creationHardware" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="playbackHardwares" section="variablemedia">
			<field id="playbackHardware" section="variablemedia" autocomplete="true" ui-type="enum" />
		</repeat>
	</section>
</record>
