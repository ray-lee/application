    <instance id="vocab-socialmediatype">
        <web-url>socialmediatype</web-url>
        <title-ref>socialmediatype</title-ref>
        <title>Social Media Types</title>
        <options>
            <option id="facebook">Facebook</option>
            <option id="flickr">Flickr</option>
