<record id="collection-object" is-extension="true">
	<services-record-path id="mmi">collectionobjects_mmi:http://collectionspace.org/services/collectionobject/local/mmi,collectionobjects_mmi </services-record-path>

	<section id="identificationInformation">
		<repeat id="otherNumberList/otherNumber">
			<!-- Change to term list -->
			<field id="numberType" autocomplete="true" ui-type="enum" />
		</repeat>

		<field id="administrativeRemarks" section="mmi" />
		<field id="provenance" section="mmi" />
		<field id="objectTitle" mini="summary,list" section="mmi" />
		<field id="initialCatalogingCompleted" section="mmi" datatype="boolean" />
		<field id="approvedByCurator" section="mmi" datatype="boolean" />
		<field id="additionalResearchRequired" section="mmi" datatype="boolean" />
		<field id="artifactClass" autocomplete="true" ui-type="enum" section="mmi" />
		<field id="workType" autocomplete="true" ui-type="enum"  section="mmi" />
		<field id="caption" section="mmi" />
	</section>

	<section id="descriptionInformation">
		<section id="contentInformation">
			<repeat id="contentPlaces">
				<!-- Add autocomplete -->
				<field id="contentPlace" autocomplete="true" />
			</repeat>
		</section>

		<repeat id="mmiInscriptionContents" section="mmi">
			<field id="mmiInscriptionContent" section="mmi" />
		</repeat>

		<repeat id="contentEntityList/contentEntityGroup" section="mmi">
			<field id="contentEntity" autocomplete="true" section="mmi" />
			<field id="contentEntityType" section="mmi" />
			<field id="contentEntityNote" section="mmi" />
		</repeat>

		<!--
		This repeatable field group conflicts with the "contentWorksGroupList/contentWorksGroup" of
		the Variable Media schema extension.
		List/Group container has been renamed to "mmiContentWorkList/mmiContentWorkGroup"
		-->
		<repeat id="contentWorkList/contentWorkGroup" section="mmi">
			<field id="contentWork" autocomplete="true" section="mmi" />
			<field id="contentWorkType" autocomplete="true" ui-type="enum" section="mmi" />
			<field id="contentWorkNote" section="mmi" />
		</repeat>
	</section>

	<section id="objectProductionInformation">
		<repeat id="objProdEntityList/objProdEntityGroup" section="mmi">
			<field id="objProdEntity" autocomplete="true" section="mmi" />
			<field id="objProdEntityRole" autocomplete="true" ui-type="enum" section="mmi" />
			<field id="objProdEntityNote" section="mmi" />
		</repeat>

		<repeat id="mmiTechniques" section="mmi">
			<field id="mmiTechnique" section="mmi" autocomplete="true" ui-type="enum" />
		</repeat>

		<repeat id="copyrightStatements" section="mmi">
			<field id="copyrightStatement" section="mmi" />
		</repeat>

		<repeat id="formats" section="mmi">
			<field id="format" autocomplete="true" ui-type="enum" section="mmi" />
		</repeat>

		<repeat id="mmiMaterials" section="mmi">
			<field id="mmiMaterial" autocomplete="true" ui-type="enum" section="mmi" />
		</repeat>
	</section>

	<section id="objectHistoryAssociationInformation">
		<section id="associationInformation">
			<repeat id="assocOrganizationGroupList/assocOrganizationGroup">
				<!-- Change to term list -->
				<field id="assocOrganizationType" autocomplete="true" ui-type="enum" />
			</repeat>
		</section>

		<field id="historicalnotesources" section="mmi" />
	</section>
</record>
