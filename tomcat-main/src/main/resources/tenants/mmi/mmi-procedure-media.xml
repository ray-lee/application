<record id="media" is-extension="true">
	<services-record-path id="mmi">media_mmi:http://collectionspace.org/services/media/local/mmi,media_mmi</services-record-path>

	<section id="mediaInformation">
		<field id="isPrimary" datatype="boolean" section="mmi" />
		<field id="isOriginalMedia" datatype="boolean" section="mmi" />
		<field id="mediaStatus" section="mmi" />
		<field id="mediaView" autocomplete="true" ui-type="enum" section="mmi" />
	</section>
</record>
