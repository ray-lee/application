    <section id="objectRequirementInformation">
        <field id="tempRequirement" section="anthropology" autocomplete="vocab-temperaturelevel" ui-type="enum"/>
        <field id="humidityRequirement" section="anthropology" autocomplete="vocab-humiditylevel" ui-type="enum"/>
        <field id="lightRequirement" section="anthropology" autocomplete="vocab-lightlevel" ui-type="enum"/>
    </section>
</record>
