<record id="claim" is-extension="true">
	<services-record-path id="anthropology">claims_anthropology:http://collectionspace.org/services/claim/domain/anthropology,claims_anthropology</services-record-path>

	<section id="claimInformation">
		<!--<field id="claimName" section="anthropology" in-title="yes"></field>-->
		<field id="claimName" ui-search="repeatable" section="anthropology" in-title="yes" mini="summary,search,list,relate"></field>

		<repeat id="claimTypeList" section="anthropology" services-type-anonymous="false">
			<field id="claimType" ui-search="repeatable" section="anthropology" seperate_ui_container="true">
				<options>
					<option id="" default="yes">(none selected)</option>
					<option id="affiliatedHsr">affiliated human skeletal remains (HSR)</option>
					<option id="unaffiliatedCuiHsr">unaffiliated human skeletal remains (HSR) (=CUI)</option>
					<option id="afo">associated funerary object (AFO)</option>
					<option id="ufo">unassociated funerary object (UFO)</option>
					<option id="sacredObject">sacred object</option>
					<option id="objectOfCulturalPatrimony">object of cultural patrimony</option>
					<option id="nonNagpra">not subject to NAGPRA</option>
					<option id="needsFurtherResearch">needs further research</option>
					<option id="unknown">unknown</option>
				</options>
			</field>
		</repeat>

		<repeat id="claimAltNameGroupList/claimAltNameGroup" section="anthropology">
			<field id="claimAltName" section="anthropology"></field>
			<field id="claimAltNote" section="anthropology"></field>
		</repeat>

		<repeat id="claimNoteList" section="anthropology" services-type-anonymous="false">
			<field id="claimNote" section="anthropology"></field>
		</repeat>
	</section>

	<section id="claimAssociationInformation">
		<repeat id="claimInventoryInvolvedGroupList/claimInventoryInvolvedGroup" section="anthropology">
			<field id="inventoryInvolvedName" section="anthropology" autocomplete="true" ui-type="enum">
				<options/>
			</field>
			<field id="inventoryInvolvedNote" section="anthropology"></field>
		</repeat>

		<repeat id="claimSiteInvolvedGroupList/claimSiteInvolvedGroup" section="anthropology">
			<field id="claimSiteInvolvedName" section="anthropology" autocomplete="place-place"></field>
			<field id="claimSiteInvolvedNote" section="anthropology"></field>
		</repeat>

		<repeat id="claimGroupInvolvedGroupList/claimGroupInvolvedGroup" section="anthropology">
			<field id="groupInvolvedName" section="anthropology" autocomplete="concept-concept"></field>
			<field id="groupInvolvedNote" section="anthropology"></field>
		</repeat>
	</section>

	<section id="claimProcessingInformation">
		<repeat id="dateOfInitialResponseGroupList/dateOfInitialResponseGroup" section="anthropology">
			<field id="initialResponseDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="initialResponseNote" section="anthropology"></field>
		</repeat>

		<repeat id="sentToLocalCommitteeGroupList/sentToLocalCommitteeGroup" section="anthropology">
			<field id="sentToLocalCommitteeDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="sentToLocalCommitteeNote" section="anthropology"></field>
		</repeat>

		<repeat id="sentToOversightCommitteeGroupList/sentToOversightCommitteeGroup" section="anthropology">
			<field id="sentToOversightCommitteeDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="sentToOversightCommitteeNote" section="anthropology"></field>
		</repeat>

		<repeat id="sentToNationalNagpraGroupList/sentToNationalNagpraGroup" section="anthropology">
			<field id="sentToNationalNagpraDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="sentToNationalNagpraNote" section="anthropology"></field>
		</repeat>

		<repeat id="dateOfNationalNagpraApprovalGroupList/dateOfNationalNagpraApprovalGroup" section="anthropology">
			<field id="dateOfNationalNagpraApprovalDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="dateOfNationalNagpraApprovalNote" section="anthropology"></field>
		</repeat>

		<repeat id="dateOfThirtyDayNoticeGroupList/dateOfThirtyDayNoticeGroup" section="anthropology">
			<field id="dateOfThirtyDayNoticeDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="dateOfThirtyDayNoticeType" section="anthropology" seperate_ui_container="true">
				<options>
					<option id="begin">period begins</option>
					<option id="end">period ends</option>
				</options>
			</field>
			<field id="dateOfThirtyDayNoticeNote" section="anthropology"></field>
		</repeat>

		<repeat id="anthroTransferGroupList/anthroTransferGroup" section="anthropology">
			<field id="transferDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="transferNote" section="anthropology"></field>
		</repeat>

		<repeat id="recommendationOfLocalCommitteeGroupList/recommendationOfLocalCommitteeGroup" section="anthropology">
			<field id="recommendationOfLocalCommitteeDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="recommendationOfLocalCommitteeNote" section="anthropology"></field>
		</repeat>

		<repeat id="recommendationOfOversightCommitteeGroupList/recommendationOfOversightCommitteeGroup" section="anthropology">
			<field id="recommendationOfOversightCommitteeDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="recommendationOfOversightCommitteeNote" section="anthropology"></field>
		</repeat>

		<repeat id="responseFromNationalNagpraGroupList/responseFromNationalNagpraGroup" section="anthropology">
			<field id="responseFromNationalNagpraDate" section="anthropology" ui-type="date" datatype="date"></field>
			<field id="responseFromNationalNagpraNote" section="anthropology"></field>
		</repeat>

		<repeat id="claimPeriodInvolvedGroupList/claimPeriodInvolvedGroup" section="anthropology">
			<group id="claimPeriodInvolvedDateGroup" section="anthropology" ui-type="groupfield/structureddate" primarykey="dateDisplayDate" ui-func="cspace.structuredDate">
				<!-- These fields are defined here as a workaround to CSPACE-5085. See http://issues.collectionspace.org/browse/CSPACE-5085 for details. -->
				<field id="dateDisplayDate" section="anthropology"/>
				<field id="dateAssociation" section="anthropology"/>
				<field id="dateEarliestSingleYear" section="anthropology"/>
				<field id="dateEarliestSingleMonth" section="anthropology"/>
				<field id="dateEarliestSingleDay" section="anthropology"/>
				<field id="dateEarliestSingleEra" section="anthropology"/>
				<field id="dateEarliestSingleCertainty" section="anthropology"/>
				<field id="dateEarliestSingleQualifier" section="anthropology"/>
				<field id="dateEarliestSingleQualifierValue" section="anthropology"/>
				<field id="dateEarliestSingleQualifierUnit" section="anthropology"/>
				<field id="dateLatestYear" section="anthropology"/>
				<field id="dateLatestMonth" section="anthropology"/>
				<field id="dateLatestDay" section="anthropology"/>
				<field id="dateLatestEra" section="anthropology"/>
				<field id="dateLatestCertainty" section="anthropology"/>
				<field id="dateLatestQualifier" section="anthropology"/>
				<field id="dateLatestQualifierValue" section="anthropology"/>
				<field id="dateLatestQualifierUnit" section="anthropology"/>
				<field id="datePeriod" section="anthropology"/>
				<field id="dateNote" section="anthropology"/>
				<field id="dateEarliestScalarValue" section="anthropology"/>
				<field id="dateLatestScalarValue" section="anthropology"/>
				<field id="scalarValuesComputed" section="anthropology"/>
			</group>
			<field id="claimPeriodInvolvedNote" section="anthropology"/>
		</repeat>
		<!-- ADD CHECKLIST PAIRS -->
		<!-- ADD CULTURAL CARE NOTES -->
	</section>
</record>
