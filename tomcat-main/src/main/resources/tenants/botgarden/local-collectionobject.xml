		</field>
		<field id="flowersJul" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersAug" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersSep" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
			</options>
		</field>
		<field id="flowersOct" section="botgarden">
			<options>
