			</options>
		</field>
		<field id="fruitsJan" datatype="boolean" section="botgarden"/>
		<field id="fruitsFeb" datatype="boolean" section="botgarden"/>
		<field id="fruitsMar" datatype="boolean" section="botgarden"/>
		<field id="fruitsApr" datatype="boolean" section="botgarden"/>
		<field id="fruitsMay" datatype="boolean" section="botgarden"/>
		<field id="fruitsJun" datatype="boolean" section="botgarden"/>
		<field id="fruitsJul" datatype="boolean" section="botgarden"/>
		<field id="fruitsAug" datatype="boolean" section="botgarden"/>
		<field id="fruitsSep" datatype="boolean" section="botgarden"/>
		<field id="fruitsOct" datatype="boolean" section="botgarden"/>
		<field id="fruitsNov" datatype="boolean" section="botgarden"/>
		<field id="fruitsDec" datatype="boolean" section="botgarden"/>
	</section>
		
	<section id="objectCollectionInformation">
		<repeat id="fieldCollectors">
			<!-- Add ui-search, and change autocomplete to organization-collector -->
			<field id="fieldCollector" autocomplete="organization-collector" ui-search="repeatable"/>
		</repeat>
		<!-- Change autocomplete -->
		<field id="fieldCollectionNumberAssignor" autocomplete="person-person,organization-collector"/>
	</section>

	<section id="localityInformation">
		<repeat id="localityGroupList/localityGroup">
			<!-- Change to dynamic lists -->
		    <field id="fieldLocCounty" autocomplete="vocab-county" ui-type="enum"><options/></field>
			<field id="fieldLocState" autocomplete="vocab-state" ui-type="enum"><options/></field>
			<field id="fieldLocCountry" autocomplete="vocab-country" ui-type="enum"><options/></field>
			
			<!-- Configure decimalLatitude and decimalLongitude to be computed -->
			<field id="decimalLatitude" ui-type="computed" ui-func="cspace.georef.dms2decimal" ui-args="vLatitude" ui-readonly="true"/>
			<field id="decimalLongitude" ui-type="computed" ui-func="cspace.georef.dms2decimal" ui-args="vLongitude" ui-readonly="true"/>
		</repeat>
	</section>

	<section id="accessionUseInformation">
		<repeat id="accessionUseGroupList/accessionUseGroup">
			<!-- Add autocomplete -->
			<field id="usedIn" autocomplete="concept-class_ca,concept-research_ca"/>
		</repeat>
	</section>
</record>