<record id="collection-object">
	<services-record-path id="botgarden">collectionobjects_botgarden:http://collectionspace.org/services/collectionobject/local/botgarden,collectionobjects_botgarden</services-record-path>
	
	<section id="coreInformation">
		<!-- Remove mini -->
		<field id="nametitle" mini=""/>
	</section>
	
	<section id="identificationInformation">
		<field id="objectNumber">
			<options>
				<!-- Add UC and JEPS id generators -->
				<option id="uc-accession" sample="UC100000">UC Accession</option>
				<option id="jeps-accession" sample="JEPS100000">Jepson Accession</option>
			</options>
		</field>

		<repeat id="otherNumberList/otherNumber">
			<field id="numberType">
				<options>
					<option id="" default="yes">Please select a value</option>
					<option id="Unknown">Unknown</option>
					<option id="Collector Number">Collector Number</option>
					<option id="Exsiccatae Number">Exsiccatae Number</option>
					<option id="GenBank Code">GenBank Code</option>
					<option id="Old institution accession number">Old Institution Accession Number</option>
					<option id="Other Label Number">Other Label Number</option>
					<option id="U.C. Botanical Garden Accession Number">U.C. Botanical Garden Accession Number</option>
				</options>
			</field>
		</repeat>
		<field id="collection">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="Unknown">Unknown</option>
				<option id="Jepson Herbarium">Jepson Herbarium</option>
				<option id="University of California Herbarium">University Herbarium, University of California, Berkeley (UC)</option>
				<option id="University of California- Santa Barbara">University of California- Santa Barbara</option>
				<option id="Herbarium (UCSC)">Herbarium (UCSC)</option>
				<option id="University of California">University of California</option>
				<option id="DHN">DHN</option>
			</options>
		</field>
		<field id="recordStatus">
		  <options>
			<option id="none" default="yes">None</option>
			<option id="partial">Partial</option>
			<option id="complete">Complete</option>
		  </options>
		</field>
		<repeat id="taxonomicIdentGroupList/taxonomicIdentGroup">
			<!-- Change organization-organization to organization-group_org -->
			<field id="identBy" autocomplete="person-person,organization-group_org"/>
		</repeat>
		<field id="labelHeader" autocomplete="concept-concept"/>
		<field id="labelFooter" autocomplete="concept-concept"/>
		
		<repeat id="typeSpecimenGroupList/typeSpecimenGroup">
			<!-- Change autocomplete -->
			<field id="typeSpecimenAssertionBy" autocomplete="person-person,organization-group_org"/>
		</repeat>
	
		<!-- Local fields -->
		
		<field id="deadFlag" datatype="boolean" section="botgarden"/>
		<field id="deadDate" ui-type="date" section="botgarden"/>
	</section>
	
	<section id="descriptionInformation">
		<field id="phase">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="Unknown">Unknown</option>
				<option id="Cone">Cone</option>
				<option id="Flowering">Flowering</option>
				<option id="Flowering/Fruiting">Flowering/Fruiting</option>
				<option id="Fruiting">Fruiting</option>
				<option id="Gametophyte">Gametophyte</option>
				<option id="Juvenile">Juvenile</option>
				<option id="Spores/Sporangia">Spores/Sporangia</option>
				<option id="Sporophyte">Sporophyte</option>
				<option id="Tetrasporophyte">Tetrasporophyte</option>
				<option id="Vegetative (non-reproductive)">Vegetative (non-reproductive)</option>
			</options>
		</field>
		<repeat id="forms">
			<field id="form">
				<options>
					<option id="" default="yes">Please select a value</option>
					<option id="Unknown">Unknown</option>
					<option id="Mounted on Paper">Mounted on Paper</option>
					<option id="Stored in a Box or Bag">Stored in a Box or Bag</option>
					<option id="Preserved in Liquid">Preserved in Liquid</option>
					<option id="Microscope Slide Preparation">Microscope Slide Preparation</option>
					<option id="Photograph">Photograph</option>
					<option id="Illustration">Illustration</option>
					<option id="Photocopy">Photocopy</option>
					<option id="Box">Box</option>
					<option id="Bag">Bag</option>
					<option id="Map">Map</option>
				</options>			
			</field>
		</repeat>
		<repeat id="annotationGroupList/annotationGroup">
			<!-- Add groups to autocomplete -->
			<field id="annotationAuthor" autocomplete="person-person,organization-group_org"/>
		</repeat>
		<field id="handwritten" section="botgarden">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="Unknown">Unknown</option>
				<option id="yes">Yes</option>
				<option id="no">No</option>
			</options>
		</field>
		<field id="postToPublic" section="botgarden">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="no" default="yes">No</option>
				<option id="yes">Yes</option>
			</options>
		</field>
		
		<field id="cbdRestriction" datatype="boolean" section="botgarden"/>

		<field id="flowerColor" section="botgarden"/>
		<field id="fruitColor" section="botgarden"/>
		<field id="propagationHistory" section="botgarden">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="individual wild plant">individual wild plant</option>
				<option id="sexual reproduction">sexual reproduction</option>
				<option id="open breeding">open breeding</option>
				<option id="controlled breeding">controlled breeding</option>
				<option id="isolated self-polinated">isolated self-polinated</option>
				<option id="derived asexually">derived asexually</option>
				<option id="vegetative reproduction">vegetative reproduction</option>
				<option id="apomictic cloning">apomictic cloning</option>
				<option id="unknown">unknown</option>
			</options>
		</field>
		<field id="breedingSystem" section="botgarden">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="male">male</option>
				<option id="female">female</option>
				<option id="both">both</option>
				<option id="dioecious and unknown sex">dioecious and unknown sex</option>
				<option id="monoecious and self-compatible">monoecious and self-compatible</option>
				<option id="monoecious and self-incompatible">monoecious and self-incompatible</option>
				<option id="agamospermy">agamospermy</option>
				<option id="unknown">unknown</option>
			</options>
		</field>
		<field id="classUse" datatype="boolean" section="botgarden"/>
		<field id="researchUse" datatype="boolean" section="botgarden"/>
		<field id="ignoreRedDot" datatype="boolean" section="botgarden"/>
		<field id="fragrance" datatype="boolean" section="botgarden"/>
		<field id="flowersJan" section="botgarden">
			<options>
				<option id="No" default="yes">No</option>
				<option id="Some">Some</option>
				<option id="Many">Many</option>
