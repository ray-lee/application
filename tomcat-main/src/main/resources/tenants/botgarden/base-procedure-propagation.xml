<record id="propagation" in-findedit="yes" type="record,procedure">
                <services-url>propagations</services-url>
                <services-tenant-plural>Propagations</services-tenant-plural>
                <services-tenant-singular>Propagation</services-tenant-singular>
                <services-list-path>abstract-common-list/list-item</services-list-path>
                <services-record-path>propagations_common:http://collectionspace.org/services/propagation,propagations_common</services-record-path>
                <services-record-path id="collectionspace_core">collectionspace_core:http://collectionspace.org/collectionspace_core/,collectionspace_core</services-record-path>
                <hassoftdelete>true</hassoftdelete>
                <structures>
                    <structure id="search">
                        <view>
                            <label-section id="messagekeys" show="true"></label-section>
                        </view>
                    </structure>
                    <structure id="screen">
                        <view>
                            <titlebar show="false"></titlebar>
                            <sidebar show="false">
                                <repeat id="termsUsed" show="true"></repeat>
                                <repeat id="relatedProcedures" show="true"></repeat>
                                <repeat id="relatedObjects" show="true"></repeat>
                            </sidebar>
                            <edit-section show="true"></edit-section>
                            <list-section show="false"></list-section>
                            <label-section id="messagekeys" show="true"></label-section>
                        </view>
                        <repeat id="termsUsed" asSibling="true">
                            <selector>recordList-row:</selector>
                            <field id="linktext" ui-type="linktext">
                                <selector>related-number</selector>
                            </field>
                            <field id="recordtype" ui-type="list">
                                <selector>related-recordtype</selector>
                            </field>
                            <field id="sourceFieldName" ui-type="list">
                                <selector>related-field</selector>
                            </field>
                        </repeat>
                    </structure>
                    <structure id="tab">
                        <view>
                            <titlebar show="false"></titlebar>
                            <sidebar show="false"></sidebar>
                            <edit-section id="details" show="true"></edit-section>
                            <list-section id="list" show="false"></list-section>
                            <label-section id="messagekeys" show="true"></label-section>
                        </view>
                        <repeat id="list" asSibling="true">
                            <selector>recordList-row:</selector>
                            <field id="summary" ui-type="list">
                                <selector>related-summary</selector>
                            </field>
                            <field id="number" ui-type="list">
                                <selector>related-number</selector>
                            </field>
                        </repeat>
                    </structure>
                </structures>
				
                <!-- <include src="domain-procedure-propagation.xml" strip-root="yes"/> -->
				
                <row-selector>.csc-recordList-row:</row-selector>
                <section id="coreInformation">
                    <field id="tenantID" section="collectionspace_core" attributes="GET"></field>
                    <field id="createdBy" section="collectionspace_core" attributes="GET"></field>
                    <field id="updatedBy" ui-search="repeatable" section="collectionspace_core" attributes="GET"></field>
                    <field id="createdAt" section="collectionspace_core" attributes="GET"></field>
                    <field id="updatedAt" ui-search="range" ui-type="date" section="collectionspace_core" attributes="GET" mini="search,list,relate"></field>
                </section>
                <section id="propagationInformation">
                    <field id="propNumber" ui-search="repeatable" in-title="yes" chooser="yes" mini="number,search,list,terms,relate" seperate_ui_container="true">
                        <title-selector>titleBar-propNumber</title-selector>
                        <selector>propagation-propNumber</selector>
                        <container-selector>propagation-propNumber-patternChooserContainer</container-selector>
            			<options>
            				<option id="prop-number" sample="UCBG1000">Propagation Number</option>
            			</options>
            		</field>
                    <field id="propDate" ui-type="groupfield/structureddate" 
                        primarykey="propDate.dateDisplayDate" ui-func="cspace.structuredDate"/>
                    <field id="propReason" ui-search="repeatable" >
                        <options>
                            <option id="" default="yes">Please select a value</option>
                            <option id="foo">foo</option>
                            <option id="bar">bar</option>
                        </options>
                    </field>
                    <field id="propType" ui-search="repeatable">
                        <options>
                            <option id="" default="yes">Please select a value</option>
                            <option id="seeds">Seeds</option>
                            <option id="cutting">Cutting</option>
                            <option id="plant">Plant</option>
                        </options>
                    </field>
                    <field id="propComments"/>
                    <field id="numStarted"/>
                    <repeat id="scarStratGroupList/scarStratGroup">
                        <field id="scarStrat">
                            <options>
                                <option id="" default="yes">Please select a value</option>
                                <option id="foo">foo</option>
                                <option id="bar">bar</option>
                            </options>
                        </field>
                        <field id="duration"/>
                        <field id="durationUnit">
                            <options>
                                <option id="" default="yes">Please select a value</option>
                                <option id="days">Days</option>
                                <option id="weeks">Weeks</option>
                                <option id="months">Months</option>
                            </options>
                        </field>
                    </repeat>                    
                    <field id="extraSeeds" datatype="boolean"/>
                    <field id="spores" datatype="boolean"/>
                    <field id="cuttingType">
                        <options>
                            <option id="" default="yes">Please select a value</option>
                            <option id="foo">foo</option>
                            <option id="bar">bar</option>
                        </options>
                    </field>
                    <field id="hormone">
                        <options>
                            <option id="" default="yes">Please select a value</option>
                            <option id="foo">foo</option>
                            <option id="bar">bar</option>
                        </options>
                    </field>
                    <field id="concentration"/>
                    <field id="wounded" datatype="boolean"/>
                    <field id="plantType">
                        <options>
                            <option id="" default="yes">Please select a value</option>
                            <option id="foo">foo</option>
                            <option id="bar">bar</option>
                        </options>
                    </field>
                    <field id="germinationDate" ui-type="date"/>
                    <field id="successRate"/>
                    
                    <repeat id="propActivityGroupList/propActivityGroup">
                        <field id="order"/>
                        <!-- <field id="activityDate" ui-type="date"/> -->
                        <group id="activityDate" ui-type="groupfield/structureddate" 
                            primarykey="dateDisplayDate" ui-func="cspace.structuredDate">
                        </group>
                        <field id="activityType">
                            <options>
                                <option id="" default="yes">Please select a value</option>
                                <option id="foo">foo</option>
                                <option id="bar">bar</option>
                            </options>
                        </field>
                        <field id="propCount"/>
                        <field id="medium" autocomplete="concept-concept"/>
                        <field id="potSize">
                            <options>
                                <option id="" default="yes">Please select a value</option>
                                <option id="foo">foo</option>
                                <option id="bar">bar</option>
                            </options>
                        </field>
                        <field id="nurseryLocation" autocomplete="location-location"/>
                        <field id="conditions"/>
                        <field id="chemicalApplied">
                            <options>
                                <option id="" default="yes">Please select a value</option>
                                <option id="foo">foo</option>
                                <option id="bar">bar</option>
                            </options>
                        </field>
                        <field id="activityConcentration"/>
                        <field id="activityComments"/>
                    </repeat>
                </section>
            </record>